<?xml version="1.0" encoding="UTF-8"?>
<faces-config
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
        version="2.0">

    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>welcomUser</from-outcome>
            <to-view-id>/welcomUser.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>welcomAdmin</from-outcome>
            <to-view-id>/welcomAdmin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nosuchuser</from-outcome>
            <to-view-id>/unknown.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/components/menu.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>noaccess</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>accessOk</from-outcome>
            <to-view-id>/buyTicket.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>